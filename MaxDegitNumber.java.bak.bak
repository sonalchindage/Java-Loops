class MaxDegitNumber 
{
	public static void main(String[] args) 
	{
		int num = 1237459;
		int max=0;
		int min=9;
		
	  for(int i= num; i>0; i/=10)
		{
			int rem=num%10;
			if(max < rem)
			{
				max=rem;
			}
			else if(min>rem)
			{
				rem=min;
			}
		}
	  System.out.println("largest digit from="+num+":"+max);
	   System.out.println("largest digit from="+num+":"+min);
	}
}
